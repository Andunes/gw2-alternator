<Application x:Class="guildwars2.tools.alternator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             DispatcherUnhandledException="App_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="theme/DefaultTheme.xaml"/>
                <ResourceDictionary Source="theme/MenuButtonTheme.xaml"/>
                <ResourceDictionary Source="theme/FadeOutButtonTheme.xaml"/>
                <ResourceDictionary Source="theme/CheckBoxTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
